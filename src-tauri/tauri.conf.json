{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "trupi-stresser",
  "version": "0.1.0",
  "identifier": "com.trupi-stresser.app",
  "build": {
    "beforeDevCommand": "npm run start",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist/trupi-stresser/browser"
  },
  "app": {
    "windows": [
      {
        "title": "trupi-stresser",
        "resizable": false,
        "fullscreen": false,
        "width": 852,
        "height": 600
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        "default",
        "window"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
